<div class="composer-column">
  <% group.each do |letter, composers| %>
    <div class="composer-letter-group">
      <a name="<%= letter.downcase %>"</a>
      <p class="letter-header"> <%= letter %>  </p>
      <% composers.each do |composer| %>
        <p><%= composer_link(composer) %></p>
      <% end %>
    </div>
  <% end %>
</div>
